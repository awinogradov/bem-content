block('list') (
    tag()('ul'),

    content()(function() {
        var ctx = this.ctx;

        return (ctx.items || []).map(function(item, i) {
            return [
                !!i,
                {
                    elem : 'item',
                    content: item
                }
            ];
        });
    }),

    elem('item').tag()('li')
)
